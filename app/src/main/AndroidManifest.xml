<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pattimura.wims">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/wims_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
        <activity
            android:name=".HalamanLogin"
            android:label=""
            android:parentActivityName=".HalamanAwal" />
        <activity
            android:name=".LandingPage"
            android:label=""
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity android:name=".HalamanAwal">

        </activity>
        <activity
            android:name=".HalamanDaftar"
            android:label=""
            android:parentActivityName=".HalamanAwal" />
        <activity
            android:name=".HalamanUbahProfil"
            android:label=""
            android:parentActivityName=".LandingPage" />
        <activity
            android:name=".UbahDetailProfile"
            android:label=""

            android:parentActivityName=".LandingPage" />

        <activity
            android:name=".PesanPersonal"
            android:label=""

            android:parentActivityName=".LandingPage" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>